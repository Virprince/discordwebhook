{
    "name": "virprince/discordwebhook",
    "description": "Link your bolt content with discord webhook",
    "type": "bolt-extension",
    "version": "v0.0.1",
    "keywords": [
        "foo",
        "bar"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Virprince",
            "email": "virprince@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Virprince\\DiscordWebHook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Virprince\\DiscordWebHook\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Virprince\\DiscordWebHook\\DiscordWebHookExtension",
        "bolt-icon": "bolt-icon.png",
        "bolt-screenshots": [
            "screenshots/screenshot1.png"
        ]
    }
}
